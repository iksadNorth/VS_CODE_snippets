{
	// Place your snippets for jsp here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	"JSP FILE SNIPPET": {
		"prefix": ["jsp", "<%@"],
		"body": [
			"<%@ taglib prefix=\"c\" uri=\"http://java.sun.com/jsp/jstl/core\" %>",
			"<%@ taglib prefix=\"x\" uri=\"http://java.sun.com/jsp/jstl/xml\" %>",
			"<%@ taglib prefix=\"fmt\" uri=\"http://java.sun.com/jsp/jstl/fmt\" %>",
			"<%@ taglib prefix=\"sql\" uri=\"http://java.sun.com/jsp/jstl/sql\" %>",
			"<%@ taglib prefix=\"fn\" uri=\"http://java.sun.com/jsp/jstl/functions\" %>",
			"<%@ page language=\"java\" contentType=\"text/html; charset=UTF-8\" pageEncoding=\"UTF-8\"%>",
			"",
			"<!DOCTYPE html>",
			"<html>",
			"",
			"<head>",
			"<meta charset='UTF-8'>",
			"<title>$TM_FILENAME_BASE</title>",
			"</head>",
			"",
			"<body>",
			"",
			"",
			"</body>",
			"",
			"</html>"
		],
		"description": "My Jsp Snippet"
	},

	"isNull": {
		"prefix": ["empty", "isNull"],
		"body": [
			"empty"
		],
		"description": "${empty Object}"
	},

	"pageContext.request.contextPath": {
		"prefix": ["pageContext.request.contextPath"],
		"body": [
			"pageContext.request.contextPath"
		],
		"description": "index.jsp를 호출하는 기본 경로"
	},

	"JSTL if": {
		"prefix": "if",
		"body": [
			"<c:if test=\"\">",
			"\t",
			"</c:if>"
		],
		"description": "if in jstl"
	},
	"JSTL choose": {
		"prefix": "choose",
		"body": [
			"<c:choose>",
    			"\t<c:when test=\"\"></c:when>",
    			"\t<c:otherwise></c:when>",
			"</c:choose>",
		],
		"description": "choose in jstl"
	},
	"JSTL forEach": {
		"prefix": ["foreach","forEach"],
		"body": [
			"<c:forEach var=\"item\" items=\"\">",
			"\t",
			"</c:forEach>"
		],
		"description": "forEach in jstl"
	},
	"JSTL for": {
		"prefix": "for",
		"body": [
			"<c:forEach var=\"no\" begin=\"00\" end=\"10\">",
			"\t",
			"</c:forEach>"
		],
		"description": "for in jstl"
	},
	"JSTL forTakens": {
		"prefix": ["forTakens", "fortakens"],
		"body": [
			"<c:forTakens var=\"word\" items=\"  \" delims=\"AND\">",
			"\t",
			"</c:forTakens>"
		],
		"description": "forTakens in jstl : items={'a,b,c'} & delims={','} -> {'a','b','c'}"
	},
	"JSTL url": {
		"prefix": "url",
		"body": [
			"<c:url var=\"\" value=\"\">",
			"\t<c:param name=\"\" value=\"\" />",
			"</c:url>"
		],
		"description": "url in jstl"
	},
	"JSTL import": {
		"prefix": "import",
		"body": [
			"<c:import url=\"\" />"
		],
		"description": "import in jstl"
	},
	"JSTL out": {
		"prefix": "out",
		"body": [
			"<c:out value=\"\" default=\"\"/>"
		],
		"description": "out in jstl"
	},
	"JSTL set": {
		"prefix": "set",
		"body": [
			"<c:set var=\"\" scope=\"page|request|session|application\">",
			"\t",
			"</c:set>"
		],
		"description": "set in jstl : scope에 따라 변수의 생존 범위가 결정됨. 생략 시, JspContext에 저장"
	},
	"JSTL set target": {
		"prefix": ["set target", "settarget"],
		"body": [
			"<c:set target=\"\" property=\"\" value=\"\" />"
		],
		"description": "set target in jstl : result -> (${target.property} == value)"
	},
	"JSTL remove": {
		"prefix": "remove",
		"body": [
			"<c:remove var=\"\" scope=\"page|request|session|application\" />"
		],
		"description": "remove in jstl : remove var made by 'set'"
	},
	"JSTL redirect": {
		"prefix": "redirect",
		"body": [
			"<c:redirect url=\"\" />"
		],
		"description": "redirect in jstl"
	},


	"JSTL fmt:parseDate": {
		"prefix": "fmt:parseDate",
		"body": [
			"<fmt:parseDate var=\"\" value=\"2020-02-15\" pattern=\"yyyy-MM-dd\" />"
		],
		"description": "fmt:parseDate in jstl"
	},
	"JSTL fmt:formatDate": {
		"prefix": "fmt:formatDate",
		"body": [
			"<fmt:formatDate value=\"${varDate}\" pattern=\"yyyy-MM-dd\" />"
		],
		"description": "fmt:formatDate in jstl : 날짜 객체만 다룸"
	},
}